{
  "name": "portfolio",
  "version": "0.0.0",
  "scripts": {
    "ng": "ng",
    "start": "ng serve",
    "build": "ng build",
    "build:prod": "ng build --configuration --base-href https://portfolio.pxuanbach.id.vn/",
    "watch": "ng build --watch --configuration development",
    "test": "ng test",
    "build-image": "docker build -t pxuanbach/my-portfolio-v2 .",
    "push-image": "docker push pxuanbach/my-portfolio-v2",
    "up": "docker compose up -d",
    "down": "docker compose down",
    "pull": "docker compose pull",
    "deploy:ghpages": "ng deploy --cname=portfolio.pxuanbach.id.vn --repo=https://github.com/pxuanbach/my-portfolio-v2.git"
  },
  "private": true,
  "dependencies": {
    "@angular/animations": "^17.0.0",
    "@angular/common": "^17.0.0",
    "@angular/compiler": "^17.0.0",
    "@angular/core": "^17.0.0",
    "@angular/forms": "^17.0.0",
    "@angular/platform-browser": "^17.0.0",
    "@angular/platform-browser-dynamic": "^17.0.0",
    "@angular/router": "^17.0.0",
    "@fortawesome/fontawesome-free": "^6.5.1",
    "@popperjs/core": "^2.11.8",
    "angular-cli-ghpages": "^2.0.1",
    "ngx-extended-pdf-viewer": "^19.4.0-alpha.2",
    "pdfmake": "^0.2.9",
    "rxjs": "~7.8.0",
    "zone.js": "~0.14.2"
  },
  "devDependencies": {
    "@angular-devkit/build-angular": "^17.0.10",
    "@angular/cli": "^17.0.10",
    "@angular/compiler-cli": "^17.0.0",
    "@angular/localize": "^17.0.0",
    "@types/jasmine": "~5.1.0",
    "@types/pdfmake": "^0.2.9",
    "jasmine-core": "~5.1.0",
    "karma": "~6.4.0",
    "karma-chrome-launcher": "~3.2.0",
    "karma-coverage": "~2.2.0",
    "karma-jasmine": "~5.1.0",
    "karma-jasmine-html-reporter": "~2.1.0",
    "tslib": "^2.6.2",
    "typescript": "~5.2.2"
  }
}
